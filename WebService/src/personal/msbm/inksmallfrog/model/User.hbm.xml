<!DOCTYPE hibernate-mapping SYSTEM "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="personal.msbm.inksmallfrog.model">

    <class name="User" table="MSBM_user">
        <id name="id">
            <generator class="native"/>
        </id>
        <property name="name" length="20" type="java.lang.String" not-null="true"/>
        <property name="pwd" length="80" type="java.lang.String" not-null="true"/>
        <property name="address" length="120" type="java.lang.String"/>
    </class>


</hibernate-mapping>